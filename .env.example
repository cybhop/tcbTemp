# Environment Variables Configuration
# Copy this file to .env and update the values with your actual configuration

# =============================================================================
# GMAIL API CONFIGURATION
# =============================================================================

# Gmail API Client ID from Google Cloud Console
# 1. Go to https://console.cloud.google.com/
# 2. Create a new project or select existing one
# 3. Enable Gmail API
# 4. Create credentials (OAuth 2.0 Client ID)
# 5. Set authorized redirect URIs to include your app's callback URL
GMAIL_CLIENT_ID=your_gmail_client_id_here.apps.googleusercontent.com

# Gmail API Client Secret from Google Cloud Console
# Found in the same place as Client ID
GMAIL_CLIENT_SECRET=your_gmail_client_secret_here

# OAuth2 Redirect URI
# Must match exactly what you configured in Google Cloud Console
# For development: http://localhost:3000/api/auth/google/callback
# For production: https://yourdomain.com/api/auth/google/callback
GMAIL_REDIRECT_URI=http://localhost:3000/api/auth/google/callback

# Gmail OAuth2 Refresh Token
# Generated after initial OAuth flow completion
# This allows your app to access Gmail without user re-authentication
GMAIL_REFRESH_TOKEN=your_gmail_refresh_token_here

# From Email Address
# The Gmail account that will send emails
GMAIL_FROM_EMAIL=your-email@gmail.com

# From Name
# Display name for sent emails
GMAIL_FROM_NAME=Your App Name

# =============================================================================
# AUTHENTICATION CONFIGURATION
# =============================================================================

# JWT Secret for Access Tokens
# Generate with: openssl rand -base64 32
# Used to sign and verify JWT access tokens
JWT_ACCESS_SECRET=your_jwt_access_secret_here_should_be_long_and_random

# JWT Secret for Refresh Tokens
# Generate with: openssl rand -base64 32
# Used to sign and verify JWT refresh tokens
JWT_REFRESH_SECRET=your_jwt_refresh_secret_here_should_be_different_from_access

# JWT Access Token Expiration
# Examples: 15m, 1h, 24h
JWT_ACCESS_EXPIRES_IN=15m

# JWT Refresh Token Expiration
# Examples: 7d, 30d, 90d
JWT_REFRESH_EXPIRES_IN=7d

# Password Hashing Salt Rounds
# Higher numbers = more secure but slower
# Recommended: 10-12 for production
BCRYPT_SALT_ROUNDS=10

# Email Verification Token Expiration (in minutes)
EMAIL_VERIFICATION_EXPIRES_IN=60

# Password Reset Token Expiration (in minutes)
PASSWORD_RESET_EXPIRES_IN=30

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================

# Application Base URL
# Used for generating email links and redirects
# For development: http://localhost:3000
# For production: https://yourdomain.com
APP_URL=http://localhost:3000

# Environment Mode
# Options: development, staging, production
NODE_ENV=development

# API Base URL
# Usually same as APP_URL with /api suffix
API_URL=http://localhost:3000/api

# Frontend URL (if different from APP_URL)
# Used for CORS and redirects
FRONTEND_URL=http://localhost:3000

# Application Name
# Used in email templates and metadata
APP_NAME=Your App Name

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# Database Connection String
# Examples:
# MongoDB: mongodb://localhost:27017/yourdb
# PostgreSQL: postgresql://user:password@localhost:5432/yourdb
# MySQL: mysql://user:password@localhost:3306/yourdb
DATABASE_URL="your_database_url_here"

# =============================================================================
# EMAIL SETTINGS
# =============================================================================

# SMTP Configuration (fallback if Gmail API fails)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your_app_password_here
SMTP_SECURE=false

# Email Template Settings
EMAIL_TEMPLATES_DIR=./templates/emails
EMAIL_ASSETS_URL=https://your-cdn-url.com/email-assets

# Notification Settings
SEND_WELCOME_EMAIL=true
SEND_PASSWORD_RESET_EMAIL=true
SEND_EMAIL_VERIFICATION=true
SEND_NOTIFICATION_EMAILS=true

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# Rate Limiting
# Requests per window per IP
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# API Rate Limiting
API_RATE_LIMIT_WINDOW_MS=900000
API_RATE_LIMIT_MAX_REQUESTS=1000

# Login Rate Limiting
LOGIN_RATE_LIMIT_WINDOW_MS=900000
LOGIN_RATE_LIMIT_MAX_ATTEMPTS=5

# CORS Configuration
# Comma-separated list of allowed origins
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com

# Security Headers
SECURITY_HEADERS_ENABLED=true
CONTENT_SECURITY_POLICY_ENABLED=true

# Session Configuration
SESSION_SECRET=your_session_secret_here_should_be_long_and_random
SESSION_COOKIE_SECURE=false
SESSION_COOKIE_SAME_SITE=lax

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================

# Log Level
# Options: error, warn, info, debug
LOG_LEVEL=info

# Log File Path
LOG_FILE=./logs/app.log

# Enable Console Logging
LOG_CONSOLE=true

# =============================================================================
# THIRD-PARTY SERVICES
# =============================================================================

# Google Analytics (optional)
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Sentry Error Tracking (optional)
SENTRY_DSN=your_sentry_dsn_here

# Redis Configuration (for caching and sessions)
REDIS_URL=redis://localhost:6379

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================

# Debug Mode
DEBUG=true

# Hot Reload
HOT_RELOAD=true

# Mock External APIs
MOCK_EXTERNAL_APIS=false

# =============================================================================
# PRODUCTION SETTINGS
# =============================================================================

# SSL/TLS Configuration
SSL_ENABLED=false
SSL_CERT_PATH=/path/to/ssl/cert.pem
SSL_KEY_PATH=/path/to/ssl/key.pem

# Performance Monitoring
PERFORMANCE_MONITORING_ENABLED=false

# Health Check Settings
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PATH=/health

